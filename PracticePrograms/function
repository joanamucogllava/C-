// ConsoleApplication1.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include <string>

using namespace std;

void displayMessage()
{
	cout << "Hello from the function displayMessage.\n";
}
int main()
{
	cout << "Hello from main.\n";
	displayMessage();
	cout << "Back in function main again.\n";

	system("pause");
    return 0;
}
